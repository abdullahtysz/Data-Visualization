      },
      "source": [
        "age_cat = []\n",
        "for i in df[\"age\"]:\n",
        "    age_cat.append(i)\n",
        "\n",
        "df[\"age_cat\"]=[\"below25\" if x <= 25 else \"above25\" for x in df[\"age\"]]\n"
      ],
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
